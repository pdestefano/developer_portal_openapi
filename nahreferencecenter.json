    "/v1/content/articles": {
      "get": {
        "tags": [
          "Reference Center"
        ],
        "summary": "GET a list of articles and metadata that have been updated",
        "description": "Return list of updated articles within a given range",
        "operationId": "get-v1-products-product-content-articles-updates",
        "parameters": [
          {
            "name": "updatedSince",
            "in": "query",
            "description": "Return article metadata starting at this date",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "updatedBefore",
            "in": "query",
            "description": "Return article metdata ending at this date",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Next page of items to return",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "_metadata": {
                      "type": "object",
                      "required": [
                        "totalItems",
                        "links"
                      ],
                      "properties": {
                        "totalItems": {
                          "type": "integer",
                          "description": "Total number of items  that fall within the requested dates.",
                          "example": 523
                        },
                        "totalPages": {
                          "type": "integer",
                          "description": "Total number of pages that fall within the requested dates.",
                          "example": "5"
                        },
                        "page": {
                          "type": "integer",
                          "description": "Page of items that are returned in this response.",
                          "example": "3"
                        },
                        "links": {
                          "minItems": 1,
                          "type": "array",
                          "description": "HATEOAS links for self, previous, next, first, last as available.",
                          "items": {
                            "type": "object",
                            "properties": {
                              "rel": {
                                "type": "string",
                                "description": "Relationship of the link and the current page of results.",
                                "example": "self"
                              },
                              "href": {
                                "type": "string",
                                "description": "Hypertext REFerence link.",
                                "example": "https://apis.ebsco.com/medsapi-ref-ctr/v1/articles"
                              }
                            },
                            "required": [
                              "rel",
                              "href"
                            ]
                          }
                        }
                      }
                    },
                    "items": {
                      "type": "array",
                      "description": "A list of all items matching the search request.",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "string",
                            "description": "ID of the article.",
                            "example": "t904163"
                          },
                          "slug": {
                            "type": "string",
                            "example": "/drug-monograph/calcium-acetate",
                            "description": "Slug for the article"
                          },
                          "createdAt": {
                            "type": "string",
                            "example": "2023-11-19T11:39:10.637Z",
                            "description": "When the article was created"
                          },
                          "updatedAt": {
                            "type": "string",
                            "example": "2024-04-25T14:52:33.116Z",
                            "description": "When the article was last modified"
                          },
                          "title": {
                            "type": "string",
                            "example": "Calcium Acetate",
                            "description": "Title of the article"
                          },
                          "hlType": {
                            "type": "string",
                            "example": "SkillCompetencyChecklist",
                            "description": "Disseminated Intravascular Coagulation (DIC): Managing in Patients with Cancer"
                          }
                        },
                        "required": [
                          "id",
                          "links"
                        ]
                      }
                    }
                  },
                  "required": [
                    "_metadata"
                  ]
                }
              }
            }
          }
        },
        "security": [
          {
            "BearerToken": []
          }
        ],
        "x-internal": false
      }
    }
  },
